{
  "--mbx-codebox-background": {
    "description": "Fallback value for `mbx-codebox-background-light` and `mbx-codebox-background-dark`"
  },
  "--mbx-codebox-background-light": {
    "description": "CodeBox background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-background-light",
    "fallback": "--mbx-codebox-background"
  },
  "--mbx-codebox-background-dark": {
    "description": "CodeBox background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-background-dark",
    "fallback": "--mbx-codebox-background"
  },
  "--mbx-codebox-text": {
    "description": "Fallback value for `mbx-codebox-text-light` and `mbx-codebox-text-dark`",
    "isFallback": true
  },
  "--mbx-codebox-text-light": {
    "description": "CodeBox text when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-text-light",
    "fallback": "--mbx-codebox-text"
  },
  "--mbx-codebox-text-dark": {
    "description": "CodeBox text when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-text-dark",
    "fallback": "--mbx-codebox-text"
  }
}
