{
  "--mbx-slider-background": {
    "description": "Fallback value for `mbx-slider-background-light` and `mbx-slider-background-dark`"
  },
  "--mbx-slider-background-light": {
    "description": "Slider background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-background-light",
    "fallback": "--mbx-slider-background"
  },
  "--mbx-slider-background-dark": {
    "description": "Slider background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-background-dark",
    "fallback": "--mbx-slider-background"
  },
  "--mbx-slider-thumb": {
    "description": "Fallback value for `mbx-slider-thumb-light` and `mbx-slider-thumb-dark`"
  },
  "--mbx-slider-thumb-light": {
    "description": "Slider thumb color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "default": "radial-gradient(#2d3748, #1d232e)",
    "fallback": "--mbx-slider-thumb"
  },
  "--mbx-slider-thumb-dark": {
    "description": "Slider thumb color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "default": "radial-gradient(#e3e4e9, #9b9999)",
    "fallback": "--mbx-slider-thumb"
  },
  "--mbx-slider-thumb-hover": {
    "description": "Fallback value for `mbx-slider-thumb-hover-light` and `mbx-slider-thumb-hover-dark`"
  },
  "--mbx-slider-thumb-hover-light": {
    "description": "Slider thumb color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`, on [hover](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#hover)",
    "defaultInternal": "--mbx-slider-thumb-light",
    "fallback": "--mbx-slider-thumb-hover"
  },
  "--mbx-slider-thumb-hover-dark": {
    "description": "Slider thumb color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`, on [hover](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#hover)",
    "defaultInternal": "--mbx-slider-thumb-dark",
    "fallback": "--mbx-slider-thumb-hover"
  }
}
