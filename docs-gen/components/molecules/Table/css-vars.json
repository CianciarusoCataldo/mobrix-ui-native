{
  "--mbx-table-background": {
    "description": "Fallback value for `--mbx-table-background-light` and `--mbx-table-background-dark`"
  },
  "--mbx-table-background-light": {
    "defaultGlobal": "--mbx-background-light",
    "fallback": "--mbx-table-background",
    "description": "Table background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off'"
  },
  "--mbx-table-background-dark": {
    "defaultGlobal": "--mbx-background-dark",
    "fallback": "--mbx-table-background",
    "description": "Table background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on'"
  },
  "--mbx-table-text": {
    "description": "Fallback value for `--mbx-table-text-light` and `--mbx-table-text-dark`"
  },
  "--mbx-table-text-light": {
    "defaultGlobal": "--mbx-text-light",
    "fallback": "--mbx-table-text",
    "description": "Table text color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off'"
  },
  "--mbx-table-text-dark": {
    "defaultGlobal": "--mbx-text-dark",
    "fallback": "--mbx-table-text",
    "description": "Table text color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on'"
  },
  "--mbx-table-header-background": {
    "description": "Fallback value for `--mbx-table-header-background-light` and `--mbx-table-header-background-dark`"
  },
  "--mbx-table-header-background-light": {
    "defaultColor": "linear-gradient(to right, #354155, #2b3546)",
    "fallback": "--mbx-table-header-background",
    "description": "Table header background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off'"
  },
  "--mbx-table-header-background-dark": {
    "defaultColor": "linear-gradient(to right, #4b5b77, #3e4c64)",
    "fallback": "--mbx-table-header-background",
    "description": "Table header background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on'"
  },
  "--mbx-table-header-text": {
    "description": "Fallback value for `--mbx-table-header-text-light` and `--mbx-table-header-text-dark`"
  },
  "--mbx-table-header-text-light": {
    "defaultColor": "whitesmoke",
    "fallback": "--mbx-table-header-text",
    "description": "Table header text color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off'"
  },
  "--mbx-table-header-text-dark": {
    "defaultColor": "rgb(255, 255, 255)",
    "fallback": "--mbx-table-header-text",
    "description": "Table header text color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on'"
  },
  "--mbx-table-border": {
    "description": "Fallback value for `--mbx-table-border-light` and `--mbx-table-border-dark`"
  },
  "--mbx-table-border-light": {
    "defaultColor": "darkslategrey",
    "fallback": "--mbx-table-border",
    "description": "Table border color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off'"
  },
  "--mbx-table-border-dark": {
    "default": "rgb(219, 219, 219)",
    "fallback": "--mbx-table-border",
    "description": "Table border color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on'"
  }
}
