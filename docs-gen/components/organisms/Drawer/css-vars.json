{
  "--mbx-drawer-background": {
    "description": "Fallback value for `--mbx-drawer-background-light` and `--mbx-drawer-background-dark`"
  },
  "--mbx-drawer-background-light": {
    "description": "Drawer background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-background-light",
    "fallback": "--mbx-drawer-background"
  },
  "--mbx-drawer-background-dark": {
    "description": "Drawer background when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-background-dark",
    "fallback": "--mbx-drawer-background"
  },
  "--mbx-drawer-text": {
    "description": "Fallback value for `--mbx-drawer-text-light` and `--mbx-drawer-text-dark`"
  },
  "--mbx-drawer-text-light": {
    "description": "Drawer text color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultGlobal": "--mbx-text-light",
    "fallback": "--mbx-drawer-text"
  },
  "--mbx-drawer-text-dark": {
    "description": "Drawer text color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultGlobal": "--mbx-text-dark",
    "fallback": "--mbx-drawer-text"
  },
  "--mbx-drawer-arrow": {
    "description": "Fallback value for `--mbx-drawer-arrow-light` and `--mbx-drawer-arrow-dark`"
  },
  "--mbx-drawer-arrow-light": {
    "description": "Drawer arrow color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`",
    "defaultInternal": "--mbx-drawer-text-light",
    "fallback": "--mbx-drawer-arrow"
  },
  "--mbx-drawer-arrow-dark": {
    "description": "Drawer arrow color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`",
    "defaultInternal": "--mbx-drawer-text-dark",
    "fallback": "--mbx-drawer-arrow"
  },
  "--mbx-drawer-arrow-hover": {
    "description": "Fallback value for `--mbx-drawer-arrow-hover-light` and `--mbx-drawer-arrow-hover-dark`"
  },
  "--mbx-drawer-arrow-hover-light": {
    "description": "Drawer arrow color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `off`, on [hover](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#hover)",
    "fallback": "--mbx-drawer-arrow-hover"
  },
  "--mbx-drawer-arrow-hover-dark": {
    "description": "Drawer arrow color when [dark mode](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#dark) is `on`, on [hover](https://cianciarusocataldo.github.io/mobrix-ui/docs/shared/props/#hover)",
    "fallback": "--mbx-drawer-arrow-hover"
  }
}
